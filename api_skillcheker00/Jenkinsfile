pipeline {
  agent { label 'windows' }

  stages {
    stage('Build') {
      steps {
        dir('api_skillcheker00') {
          bat 'mvn -B -DskipTests clean package'
        }
      }
    }
    stage('Test') {
      steps {
        dir('api_skillcheker00') {
          bat 'mvn -B test'
        }
      }
    }
  }
}