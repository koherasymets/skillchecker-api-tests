{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Authenticated User Response",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "name": {
      "type": ["string", "null"]
    }
  },
  "required": ["id", "email"]
}